<!DOCTYPE html>
<html ng-app="PhotographyStore">
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="https://ajax.googleapis.com/ajax/libs/angularjs/1.3.15/angular.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/angular-ui-router/0.3.1/angular-ui-router.js"></script>
    <script src="javascripts/app.js"></script>
    <script src="javascripts/photosController.js"></script>
  </head>
  <body>
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    {{1 + 1}}
    <nav>
      <a ui-sref-active="active" ui-sref="login">Login</a>
      <a ui-sref-active="active" ui-sref="signup">Sign up</a>
      <a ui-sref-active="active" ui-sref="photos">My Photos</a>
    </nav>
    <div ui-view></div>
  </body>
</html>
